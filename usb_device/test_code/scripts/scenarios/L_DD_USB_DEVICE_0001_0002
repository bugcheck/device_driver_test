###############################################################################
# Scenario    : L_DD_USB_Device_0001_0002
# Description : USB Device | Enumeration: Standard Host: Gadget Serial
###############################################################################

# Begin L_DD_USB_Device_0001_0002

01	set -x; handlerUsbDeviceSetup.sh  "create" "serial"

# Checking enumeration on the Linux PC

02	set -x; echo -e '\n\n\t Please connect cable. [ MiniB to OMAP - StdA to Linux PC ] \n\n'  &&  sleep $DELAY3  &&  $USBDEVICE_DIR_HELPER/enum.sh connectHost  &&  exit 0 || exit 1
03	set -x; export ENUM_INFO="Gadget Serial"; $USBDEVICE_DIR_HELPER/enum.sh connectHostRemote  &&  exit 0 || exit 1
04	set -x; export ENUM_INFO="Gadget Serial"; echo -e '\n\n\t Disconnect USB cable from OMAP \n\n'  &&  sleep $DELAY3 && $USBDEVICE_DIR_HELPER/enum.sh	disconnectHostRemote  &&  exit 0 || exit
05	set -x; echo -e '\n\n\t Please connect cable. [ MiniB to OMAP - StdA to Linux PC ] \n\n'  &&  sleep $DELAY3  &&  $USBDEVICE_DIR_HELPER/enum.sh connectHost  &&  exit 0 || exit 1
06	set -x; export ENUM_INFO="Gadget Serial"; $USBDEVICE_DIR_HELPER/enum.sh connectHostRemote  &&  exit 0 || exit 1
07	set -x; export ENUM_INFO="Gadget Serial"; echo -e '\n\n\t Disconnect USB cable from OMAP \n\n'  &&  sleep $DELAY3 && $USBDEVICE_DIR_HELPER/enum.sh	disconnectHostRemote  &&  exit 0 || exit
08	set -x; echo -e '\n\n\t Please connect cable. [ MiniB to OMAP - StdA to Linux PC ] \n\n'  &&  sleep $DELAY3  &&  $USBDEVICE_DIR_HELPER/enum.sh connectHost  &&  exit 0 || exit 1
09	set -x; export ENUM_INFO="Gadget Serial"; $USBDEVICE_DIR_HELPER/enum.sh connectHostRemote  &&  exit 0 || exit 1
10	set -x; export ENUM_INFO="Gadget Serial"; echo -e '\n\n\t Disconnect USB cable from OMAP \n\n'  &&  sleep $DELAY3 && $USBDEVICE_DIR_HELPER/enum.sh	disconnectHostRemote  &&  exit 0 || exit

18	set -x; handlerUsbDeviceSetup.sh  "remove" "serial"

# End L_DD_USB_Device_0001_0002
