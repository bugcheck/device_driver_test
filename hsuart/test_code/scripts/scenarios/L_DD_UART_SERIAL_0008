# Begin L_DD_UART_SERIAL_0008

0001	set -x; transferData.sh $SEND $SAMPLE_FILE "115200"  $NO_FLOW_CONTROL $DISCONNECT && exit 1 || exit 0
0002	set -x; transferData.sh $SEND $SAMPLE_FILE "3500000" $AUTOCTS_AUTORTS $DISCONNECT && exit 1 || exit 0
0002	set -x; transferData.sh $SEND $SAMPLE_FILE "921600"  $XON_XOFF        $DISCONNECT && exit 1 || exit 0

# End L_DD_UART_SERIAL_0008
