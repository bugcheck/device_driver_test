# Begin L_DD_DMA_0011_OPP1
0001	set -x; cd /sys/devices/system/cpu/cpu0/cpufreq/
0002	set -x; pwd
0003	set -x; opp1.sh
0004	set -x; echo Running on OPP1
0001	set -x; insmod $TESTMODS/dma_single_channel.ko
0002	set -x; insmod $TESTMODS/dma_priority_low.ko
0003	set -x; cat /proc/driver/dma_priority_low | grep PASS
0004	set -x; rmmod dma_priority_low
0005	set -x; rmmod dma_single_channel

# End L_DD_DMA_0011_OPP1
