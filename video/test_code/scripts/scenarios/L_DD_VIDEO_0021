# Begin "L_DD_VIDEO_0021"

COMMENT comment.sh "Verify the tv detection capability works fine"

0001 set -x; tv_detect.sh "ENABLED" "RGB565 160 120"	$VIDEOFILES"/video_qqvga_rgb_30 1" "0 0 160 120"&& exit 0 || exit 1
0002 set -x; tv_detect.sh "DISABLED" "RGB565 160 120"	$VIDEOFILES"/video_qqvga_rgb_30 1" "0 0 160 120"&& exit 0 || exit 1

# End "L_DD_VIDEO_0021"
