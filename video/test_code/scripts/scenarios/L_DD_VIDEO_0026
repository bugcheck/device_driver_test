COMMENT comment.sh "Verify different format are rendered, both pipelines will be sent alternately. The content will be cacheable and flushable"

0001_VD1_SAN set -x; imgstream.sh 1 "RGB565 160 120" "$VIDEOFILES/video_qqvga_rgb_30 0 1"			&& exit 0 || exit 1
0001_VD2_SAN set -x; imgstream.sh 2 "RGB565 160 120" "$VIDEOFILES/video_qqvga_rgb_30 0 1"			&& exit 0 || exit 1
0002_VD1_SAN set -x; imgstream.sh 1 "YUYV 176 144"   "$VIDEOFILES/video_qcif_yuv_75 0 1"			&& exit 0 || exit 1
0002_VD2_SAN set -x; imgstream.sh 2 "YUYV 176 144"   "$VIDEOFILES/video_qcif_yuv_75 0 1"			&& exit 0 || exit 1
0003_VD1_SAN set -x; imgstream.sh 1 "RGB32 160 120"  "$VIDEOFILES/my.rgb32 0 1"				&& exit 0 || exit 1
0003_VD2_SAN set -x; imgstream.sh 2 "RGB32 160 120"  "$VIDEOFILES/my.rgb32 0 1"				&& exit 0 || exit 1
0004_VD1 set -x; imgstream.sh 1 "YUYV 640 480"   "$VIDEOFILES/test.yuv 0 1" "0 0 640 480"		&& exit 0 || exit 1
0004_VD2 set -x; imgstream.sh 2 "YUYV 640 480"   "$VIDEOFILES/test.yuv 0 1" "0 0 640 480"		&& exit 0 || exit 1
0005_VD1 set -x; imgstream.sh 1 "RGB565 640 480" "$VIDEOFILES/video_vga_rgb565 0 1" "0 0 640 480"	&& exit 0 || exit 1
0005_VD2 set -x; imgstream.sh 2 "RGB565 640 480" "$VIDEOFILES/video_vga_rgb565 0 1" "0 0 640 480"	&& exit 0 || exit 1
0006_VD1 set -x; imgstream.sh 1 "YUYV 864 480"   "$VIDEOFILES/wvga_video.yuv 0 1" "0 0 864 480"	&& exit 0 || exit 1
0006_VD2 set -x; imgstream.sh 2 "YUYV 864 480"   "$VIDEOFILES/wvga_video.yuv 0 1" "0 0 864 480"	&& exit 0 || exit 1
0007_VD1 set -x; imgstream.sh 1 "YUYV 1280 720"  "$VIDEOFILES/720p.yuv 0 1" "0 0 1280 720"	&& exit 0 || exit 1
0007_VD2 set -x; imgstream.sh 2 "YUYV 1280 720"  "$VIDEOFILES/720p.yuv 0 1" "0 0 1280 720"	&& exit 0 || exit 1


